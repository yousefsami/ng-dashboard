{
  "name": "ng-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5000 --host 0.0.0.0",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "deploy": "gh-pages -d dist/ng-dashboard",
    "e2e": "ng e2e",
    "p": "npm run build:core && npm run npm:core",
    "build:core": "ng build core && npm run scss",
    "scss": "sass projects/core/src/lib/export-styles.scss dist/ng-dashboard-core/styles.css",
    "npm:core": "cd dist/ng-dashboard-core && npm publish && cd ../.."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/store": "^8.6.0",
    "@ngx-translate/core": "^11.0.1",
    "bootstrap": "^4.4.1",
    "gh-pages": "^2.1.1",
    "lottie-web": "^5.6.7",
    "moment": "^2.24.0",
    "ng-media": "^1.0.12",
    "ng2-pdf-viewer": "^6.1.3",
    "ngx-currency": "^2.2.2",
    "ngx-highlightjs": "^4.0.2",
    "ngx-image-cropper": "^3.1.5",
    "ngx-lottie": "^5.4.0",
    "ngx-universal-cookies": "^8.0.1",
    "response-type": "^1.1.5",
    "rxjs": "~6.4.0",
    "sass-material-colors": "0.0.5",
    "tslib": "^1.10.0",
    "url-matcher": "^0.2.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
